{
  "name": "item-description",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:maria && npm run test:data-gen",
    "test:maria": "cross-env NODE_ENV=test DB=mariadb jest tests/server.test.js --testTimeout=15000",
    "test:data-gen": "jest db/data-gen/* --config=db/data-gen/jest.config.js",
    "server:dev": "node server/parseEnvStartServer.js",
    "server:maria": "npm run server:dev --db=mariadb",
    "server:raven": "npm run server:dev --db=ravendb",
    "seed:maria": "cross-env NODE_ENV=development node db/seedMariaDB.js",
    "seed:raven": "cross-env NODE_ENV=development node db/seedRavenDB.js",
    "raven:data:gen": "cross-env DB=ravendb npm run data:gen --overwrite --numRecords=10000000",
    "start": "cross-env NODE_ENV=production DB=mariadb DB_IP=3.131.105.170 node server/index.js",
    "stop": "killall -SIGINT my-app",
    "build": "webpack --env.NODE_ENV=production --mode=production",
    "react:dev": "webpack --watch --env.NODE_ENV=development --mode=development",
    "data:gen": "node db/data-gen/index.js",
    "load-test:get-srvc": "k6 run -o influxdb=\"http://localhost:8086/Sdc\" server/load-testing/get1000RPS.srvc.k6.js",
    "load-test:post-srvc-desc": "k6 run -o influxdb=\"http://localhost:8086/Sdc\" server/load-testing/postDesc1000RPS.srvc.k6.js",
    "load-test:post-srvc-genre": "k6 run -o influxdb=\"http://localhost:8086/Sdc\" server/load-testing/postGenre1000RPS.srvc.k6.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesen-humblebundle/item-description.git"
  },
  "author": "Christina Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kesen-humblebundle/item-description/issues"
  },
  "homepage": "https://github.com/kesen-humblebundle/item-description#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "helmet": "^3.23.1",
    "mariadb": "^2.4.2",
    "ravendb": "^4.1.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "redis": "^3.0.2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "csv-parser": "^2.3.3",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "k6": "0.0.0",
    "nodemon": "^2.0.4",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
